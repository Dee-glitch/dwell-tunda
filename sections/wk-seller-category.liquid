    <div class="wk-modal-content">
        <div class="wk-modal-header">
            <span>Ask a Question</span>
            <span class="wk-close" id="wk-close-query">&times;</span>
        </div>
        <div class="wk-modal-body">
            <div class="mp-success mp-mrgn-btm-12">
                Query Submitted 
            </div>
            <form action="/" class="mp-ask-query-form mp-mrgn-btm-12" id="askquestion" method="post">
                <div class="cust_lgn">
                    <label id="customer_name_label">Name<span>*</span></label>
                    <input type="text" name="name" class="mp-full-width cust_name">
                </div>
                <div class="cust_lgn">
                    <label id="customer_email_label">Email<span>*</span></label>
                    <input type="text" name="customer_email" class="mp-full-width cust_email">
