                    <input type="hidden" name="seller_email" id="seller_email" class="mp-full-width" value="">
                </div>
                <div>
                    <label id="customer_title_label">Title<span>*</span></label>
                    <input type="text" name="title" id="title" class="mp-full-width">
                </div>
                <div>
                    <label id="customer_query_label">Description<span>*</span></label>
                    <textarea name="Description" id="Description" class="mp-full-width"></textarea>
                </div>
                <div>
                    <div class="mp-err"></div>
        
                        <div class="file_name clearboth"> <span></span></div>
                          <input id="wk-ask_upload_file" name="file" type="file" class="form-control input-lg hidden" accept="image/*">
                          <button type="button" class="btn btn-lg ask_upload_file mp-mrgn-btm-12 hidden" id="wk-ask_upload_file_btn">Upload File</button>
                   
                    <button type="submit" class="btn button-primary submit-ask mp-mrgn-btm-12" id="customer_submit_label">Submit</button>
                </div>
            </form>
        </div>
    </div>
</div>
